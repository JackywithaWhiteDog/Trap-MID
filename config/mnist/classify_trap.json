{
    "root_path": "./target_model/mnist/trap",
    "pretrained_path": "./pretrained",
    "dataset":{
        "name":"mnist",
        "img_path": "./data",
        "model_name":"IR18",
        "n_classes":5,
        "channel": 3,
        "height": 64,
        "width": 64
    },
    "trapdoor": {
        "alpha": 0.02,
        "beta": 0.2,
        "optimized": true,
        "step_size": 1e-2,
        "discriminator_loss": true,
        "discriminator_feat_loss": false,
        "discriminator_feat_model_loss": false
    },
    "VGG16":{
        "epochs":10,
        "batch_size":64,
        "lr":1e-2,
        "momentum":0.9,
        "weight_decay":1e-4
    },
    "IR18":{
        "epochs":10,
        "batch_size":64,
        "lr":1e-2,
        "momentum":0.9,
        "weight_decay":1e-4
    }
}
